SALiB,CAMjB;AACA;;AACD,OApBH;AAqBD,KAzCM;AA2CP,IAAA,gBA3CO,4BA2CU,KA3CV,EA2CiB;AACtB,WAAK,IAAL,GAAY,KAAK,CAAC,MAAN,CAAa,KAAb,CAAmB,CAAnB,CAAZ;AACA,UAAI,MAAK,GAAI,IAAI,UAAJ,EAAb;AAEA,MAAA,MAAM,CAAC,gBAAP,CACE,MADF,EAEE,YAAY;AACV,aAAK,WAAL,GAAmB,IAAnB;AACA,aAAK,YAAL,GAAoB,MAAM,CAAC,MAA3B;AACD,OAHD,CAGE,IAHF,CAGO,IAHP,CAFF,EAME,KANF;;AASA,UAAI,KAAK,IAAT,EAAe;AACb,YAAI,sBAAsB,IAAtB,CAA2B,KAAK,IAAL,CAAU,IAArC,CAAJ,EAAgD;AAC9C,UAAA,MAAM,CAAC,aAAP,CAAqB,KAAK,IAA1B;AACF;AACF;AACD;AA7DM;AATI,CAAf","sourcesContent":["<style scoped>\n.container img {\n  max-width: 200px;\n  max-height: 200px;\n  display: block;\n  margin: auto;\n  margin-top: 20px;\n  margin-bottom: 20px;\n}\n.title {\n  font-weight: 700;\n}\n</style>\n\n<template>\n  <div class=\"container\">\n    <p class=\"title\">Single Image Preview</p>\n    <hr />\n    <div class=\"large-12 medium-12 small-12 cell\">\n      <label\n        >File Preview\n        <input\n          type=\"file\"\n          id=\"file\"\n          accept=\"image/*\"\n          @change=\"handleFileUpload($event)\"\n        />\n      </label>\n      <img v-bind:src=\"imagePreview\" v-show=\"showPreview\" />\n      <button v-on:click=\"submitFile()\">Submit</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      file: \"\",\n      showPreview: false,\n      imagePreview: \"\",\n    };\n  },\n\n  methods: {\n    /*\n\t\t\t\tSubmits the file to the server\n\t\t\t*/\n    submitFile() {\n      /*\n\t\t\t\t\tInitialize the form data\n\t\t\t\t*/\n      let formData = new FormData();\n\n      /*\n\t\t\t\t\tAdd the form data we need to submit\n\t\t\t\t*/\n      formData.append(\"image\", this.file);\n\n      /*\n\t\t\t\t\tMake the request to the POST /single-file URL\n\t\t\t\t*/\n      let objectList;\n      // e.g. {class: 'backpack', x: 7, y: 33, w: 1043, h: 1361} - 이게 원소 하나\n      axios\n        .post(\"http://192.249.18.199:80/detect\", formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\",\n          },\n        })\n        .then(function (res) {\n          var data = res[\"data\"];\n          return data[\"data\"];\n        })\n        .catch(function () {\n          console.log(\"FAILURE!!\"); \n        }).then((datas) => {\n          objectList = datas;\n          console.log(objectList);\n          if (datas.length == 0) {\n            alert(\"감지된 상품이 없습니다!\"); \n          }\n          // var obj = objectList[1];\n          // console.log(obj[\"class\"])\n        });\n    },\n\n    handleFileUpload(event) {\n      this.file = event.target.files[0];\n      let reader = new FileReader();\n\n      reader.addEventListener(\n        \"load\",\n        function () {\n          this.showPreview = true;\n          this.imagePreview = reader.result;\n        }.bind(this),\n        false\n      );\n\n      if (this.file) {\n        if (/\\.(jpe?g|png|gif)$/i.test(this.file.name)) {\n          reader.readAsDataURL(this.file);\n        }\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}