AAAA,G,kBAEW,KAAA,CAAA,Q,uCAGP,CAPN,C,qCAQI,CApCN,C","sourcesContent":["<template>\n  <div class=\"keyword-selector\">\n    <div v-if=\"toggle1\">\n      <button id=\"내구성\" @click=\"keywordHandler\">내구성</button>\n      <div v-if=\"toggleText1\">\n        <p>{{ sentence1 }}</p>\n        <p>{{ sentence2 }}</p>\n        <p>{{ sentence3 }}</p>\n      </div>\n    </div>\n    <div v-if=\"toggle2\">\n      <button id=\"디자인\" @click=\"keywordHandler\">디자인</button>\n      <div v-if=\"toggleText2\">\n        <p>{{ sentence1 }}</p>\n        <p>{{ sentence2 }}</p>\n        <p>{{ sentence3 }}</p>\n      </div>\n    </div>\n    <div v-if=\"toggle3\">\n      <button id=\"상태\" @click=\"keywordHandler\">상태</button>\n    </div>\n    <div v-if=\"toggle4\">\n      <button id=\"alt1\" @click=\"keywordHandler\">{{ other1 }}</button>\n      <input\n        v-if=\"toggleText4\"\n        v-model=\"message4\"\n        placeholder=\"여기를 수정해보세요\"\n      />\n    </div>\n    <div v-if=\"toggle5\">\n      <button id=\"alt2\" @click=\"keywordHandler\">{{ other2 }}</button>\n      <input\n        v-if=\"toggleText5\"\n        v-model=\"message5\"\n        placeholder=\"여기를 수정해보세요\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  components: {},\n  beforeMount() {\n    this.other1 = \"키워드 1\";\n    this.other2 = \"키워드 2\";\n  },\n  data() {\n    return {\n      toggle1: true,\n      toggle2: true,\n      toggle3: true,\n      toggle4: true,\n      toggle5: true,\n      toggleText1: false,\n      toggleText2: false,\n      toggleText3: false,\n      toggleText4: false,\n      toggleText5: false,\n      sentence1: \"\",\n      sentence2: \"\",\n      sentence3: \"\",\n      other1: \"\",\n      other2: \"\",\n      message4: \"\",\n      message5: \"\",\n    };\n  },\n  methods: {\n    keywordHandler(event) {\n      const targetId = event.currentTarget.id;\n      switch (targetId) {\n        case \"내구성\":\n          this.toggleText1 = !this.toggleText1;\n          this.toggle2 = !this.toggle2;\n          this.toggle3 = !this.toggle3;\n          this.toggle4 = !this.toggle4;\n          this.toggle5 = !this.toggle5;\n          this.sentence1 = \"내구성과 관련된\";\n          this.sentence2 = \"어썸한 문장이 이곳으로\";\n          this.sentence3 = \"들어오면 됩니다!\";\n          break;\n        case \"디자인\":\n          this.toggleText2 = !this.toggleText2;\n          this.toggle1 = !this.toggle1;\n          this.toggle3 = !this.toggle3;\n          this.toggle4 = !this.toggle4;\n          this.toggle5 = !this.toggle5;\n          this.sentence1 = \"디자인과 관련된\";\n          this.sentence2 = \"어메이징한 문장이 이곳으로\";\n          this.sentence3 = \"들어오면 됩니다!!\";\n          break;\n        case \"상태\":\n          console.log(\"상태를 선택했습니다\");\n          break;\n        case \"alt1\":\n          this.toggleText4 = !this.toggleText4;\n          this.toggle1 = !this.toggle1;\n          this.toggle2 = !this.toggle2;\n          this.toggle3 = !this.toggle3;\n          this.toggle5 = !this.toggle5;\n          break;\n        case \"alt2\":\n          this.toggleText5 = !this.toggleText5;\n          this.toggle1 = !this.toggle1;\n          this.toggle2 = !this.toggle2;\n          this.toggle3 = !this.toggle3;\n          this.toggle4 = !this.toggle4;\n          break;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\np {\n  margin: 1em 1em;\n}\n.state-selector {\n  margin-top: 10px;\n}\n.keyword-selector {\n  margin-top: 30px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\nbutton {\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  appearance: none;\n\n  background: #4f95ff;\n  color: #ffffff;\n\n  margin: 0 1em;\n  padding: 0.5rem 1rem;\n\n  font-family: \"Noto Sans KR\", sans-serif;\n  font-size: 1rem;\n  font-weight: 400;\n  text-align: center;\n  text-decoration: none;\n\n  border: none;\n  border-radius: 4px;\n\n  display: inline-block;\n  width: auto;\n\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1),\n    0 2px 4px -1px rgba(0, 0, 0, 0.06);\n\n  cursor: pointer;\n\n  transition: 0.5s;\n}\n\nbutton:active,\nbutton:hover,\nbutton:focus {\n  background: #267dff;\n  outline: 0;\n}\nbutton:disabled {\n  opacity: 0.5;\n}\n</style>\n"],"sourceRoot":""}]}